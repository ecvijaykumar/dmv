# Firebase Admin SDK values for API token verification (private)
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-xxxxx@your-firebase-project-id.iam.gserviceaccount.com
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nREPLACE_ME\n-----END PRIVATE KEY-----\n"

# Firebase Web SDK values served by /api/public-config (public config)
FIREBASE_WEB_API_KEY=your-web-api-key
FIREBASE_WEB_AUTH_DOMAIN=your-firebase-project-id.firebaseapp.com
FIREBASE_WEB_PROJECT_ID=your-firebase-project-id
FIREBASE_WEB_STORAGE_BUCKET=your-firebase-project-id.appspot.com
FIREBASE_WEB_MESSAGING_SENDER_ID=1234567890
FIREBASE_WEB_APP_ID=1:1234567890:web:abcdef123456

# Optional API port
PORT=4000
